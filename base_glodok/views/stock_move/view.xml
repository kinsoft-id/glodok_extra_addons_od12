<odoo>
	<record id="action_stockmovemanual" model="ir.actions.act_window">
		<field name="name">StockMove</field>
		<field name="res_model">stock.move</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>
	<menuitem id="stockmovemanual_menu" name="StockMove" parent="stock.menu_stock_warehouse_mgmt" action="action_stockmovemanual" sequence="99"/>
	<record model="ir.ui.view" id="view_stock_move_operations_form">
		<field name="name">view_stock_move_operations.form</field>
		<field name="model">stock.move</field>
		<field name="inherit_id" ref="stock.view_stock_move_operations" />
		<field name="arch" type="xml">
			<field name="product_uom" position="after">
				<button type="object" name="set_reserved_done" icon="fa-check" string="Reserved Ok" />
				<button type="object" name="set_non_reserved_to_external" icon="fa-external-link" string="Set Not Reserved to External" />
			</field>
		</field>
	</record>

	<!-->>>>>>>>>>>>>>>>>>>>>>>>>-->
	<record id="label_pengiriman_tree" model="ir.ui.view">
		<field name="name">label.pengiriman.tree</field>
		<field name="model">stock.move</field>
		<field eval="8" name="priority"/>
		<field name="arch" type="xml">
			<tree decoration-muted="state == 'cancel'" decoration-danger="(state not in ('cancel','done')) and date > current_date" string="Moves" create="0">
				<field name="pembeli"/>
				<field name="batch"/>
				<field name="reference" string="DO Number"/>
				<field name="date"/>
				<!--<field name="picking_type_id"/>-->
				<field name="location_id" groups="stock.group_stock_multi_locations" string="From"/>
				<field name="product_id"/>
				<field name="product_uom_qty"/>
				<field name="product_uom" options="{'no_open': True, 'no_create': True}" string="Unit of Measure" groups="product.group_uom"/>
				<field name="cust_reference"/>
				<field name="nama_kurir"/>
				<field name="state"/>
			</tree>
		</field>
	</record>

	<record id="action_label_pengiriman" model="ir.actions.act_window">
		<field name="name">Label Pengiriman</field>
		<field name="res_model">stock.move</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
		<field name="view_id" eval="label_pengiriman_tree"/>
		<field name="domain">
			[
				('picking_id.picking_type_id','=',1),
				('batch','!=',''),
				('is_label_printed','=',False),
				('state','not in',['draft', 'done', 'cancel']),
			]
		</field>
<!--		<field name="search_view_id" ref="view_voucher_filter"/>-->
	</record>
	<menuitem id="label_pengiriman_menu" name="Label Pengiriman" parent="stock.menu_stock_warehouse_mgmt" action="action_label_pengiriman" sequence="101"/>
	<!--<<<<<<<<<<<<<<<<<<<<<-->
</odoo>